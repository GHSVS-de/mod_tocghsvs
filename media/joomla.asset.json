{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "license": "{{licenseLong}}",
  "assets": [
    {
			"name": "{{name}}.core",
			"uri": "{{name}}/tocGhsvs.js",
			"version": "{{version}}",
			"type": "script",
			"weight": -5000,
			"attributes": {"defer": true}
    },
		{
			"name": "{{name}}.override.template",
			"version": "{{version}}",
			"uri": "{{name}}.css",
			"type": "style",
			"weight": 500
		}
  ]
}
